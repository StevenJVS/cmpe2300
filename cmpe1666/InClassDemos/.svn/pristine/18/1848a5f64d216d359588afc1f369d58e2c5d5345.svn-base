using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
//Illustrating the use of CompareTo on integers and Strings
namespace Lecture11Demo3
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int x = 7, y = 10;
            string str1 = "Apple", str2 = "Ball";

            Console.WriteLine($"{x}.CompareTo({y}) returns {x.CompareTo(y)}");
            Console.WriteLine($"{y}.CompareTo({x}) returns {y.CompareTo(x)}");
            Console.WriteLine($"'{str1}'.CompareTo('{str2}') returns {str1.CompareTo(str2)}");
            Console.WriteLine($"'{str2}'.CompareTo('{str1}') returns {str2.CompareTo(str1)}");

        }
    }
}
